<% include partials/venues %>
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
	<link rel="stylesheet" href="stylesheets/landing.css">
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha/css/bootstrap.min.css">
	<link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha/js/bootstrap.min.js"></script>

 
<header class = "w3-container w3-teal">
	 <h1 align = "center">VENUES</h1>
</header>

 <body>
	<table class = "table table-hover table-bordered">
	
		<thead>
			<tr>
				<th>Name</th>
				<th>Location</th>
				<th>Capacity</th>
				<th>Price</th>
				<th>Contact</th>
				<th>Catering</th>
				<th>Decoration</th>
				<th>Category</th>
				<th>Description</th>
			</tr>
		</thead>
		<tbody>
			<% for (var i = 0; i < venues.length; i++) { %>
				<tr>
				<td><%= venues[i].name %></td>
				<td><%= venues[i].location %></td>
				<td><%= venues[i].capacity %></td>
				<td><%= venues[i].price %></td>
				<td><%= venues[i].contactno %></td>
				<td><%= venues[i].catering %></td>
				<td><%= venues[i].decoration %></td>
				<td><%= venues[i].category %></td>
				<td><%= venues[i].description %></td>
				</tr>    
			<% } %>
		</tbody>
	</table>
</body>

<% include partials/footer %>
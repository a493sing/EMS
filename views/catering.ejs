<% include partials/header %>
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
	<link rel="stylesheet" href="stylesheets/landing.css">
<div class="container">
 	<div class="row">
 		<div class="col-lg-12">
 			<div class="content">
 				<h1>Catering</h1>
 				<h3>Start planning your event today!!</h3>
 				
 			</div>
 		</div>
 	</div>
 </div>

 <table class = "table table-hover">
	<thead>
		<tr>
			<th>Name</th>
			<th>Description</th>
			<th>Location</th>
			<th>Price</th>
			<th>Beverages</th>
			<th>Contact</th>
		</tr>
	</thead>
	<tbody>
	   <% for (var i = 0; i < Catering.length; i++) { %>
		   <tr>
			 <td><%= Catering[i].name %></td>
			 <td><%= Catering[i].description %></td>
			 <td><%= Catering[i].location %></td>
			 <td><%= Catering[i].price %></td>
			 <td><%= Catering[i].beverages %></td>
			 <td><%= Catering[i].contactno %></td>
		   </tr>    
	   <% } %>
	</tbody>
</table>
 
 <!-- <script src="scripts/background.js"></script> -->

<% include partials/footer %>